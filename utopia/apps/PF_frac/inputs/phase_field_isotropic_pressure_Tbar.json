{
    "app" : "petsc_pressure_Tbar_isotropic_phase_field_2_rmtr",
    "output-path" : "isotropic_Tbar",
    "nx" : 20,
    "ny" : 20,
    "nz" : 2,
    "disp" : 1,
    "with_damage" : true,
    "use_dense_hessian" : false,
    "check_derivatives" : false,

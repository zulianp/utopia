    "pressure_increase_factor" : 0.1,

    "diff_controller" : {
        "spacing" : 1e-6,
        "hessian_from_grad" : false
    },
    "solver" : {
        "verbose" : true,
        "max-it" : 300,
        "atol": 1e-6,
        "linear-solver" : {
            "verbose" : false,
            "atol": 1e-9,
            "max-it": 1000
        }

<rmtr-app>

	<function>poisson</function>
	<mesh>unit-square</mesh>
	<elem-type>tri</elem-type>
	
	<boundary-conditions>
		
		<entry>
			<side>0</side>
			<value>0</value>
		</entry>

		<entry>
			<side>1</side>
			<value>0</value>
		</entry>

		<entry>
			<side>2</side>
			<value>1</value>
		</entry>

		<entry>
			<side>3</side>
			<value>1</value>
		</entry>
	
	</boundary-conditions>

	<refinements>1</refinements>
	<n-levels>2</n-levels>
	<order>1</order>
	<verbose>true</verbose>
	<!-- <use-newton>true</use-newton> -->

</rmtr-app>

			"type" : "tron",
			"max-it" : 5,
			"verbose" : true,
			"linear-solver" : {
				"type" : "direct"
			}
		},
		"space": {
			"system-name": "contact_sol",
			"variables": [
				{
					"name": "disp_x",
					"order" : 2

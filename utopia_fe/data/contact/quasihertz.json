{
	"contact-problem": {
		"contact-stress" : true,
		"use-amg": false,
		"use-ssnewton": true,
		"dt": 0.2,
		"is-steady" : true,
		"use-gmres" : true,
		"outer-loop-tol" : 1,
		"mesh": {
			"type": "file",
			"path_0" : "../data/contact/contact_cubes.e",
			"path_1" : "../data/contact/signorini_79207.e",
			"path_2" : "../data/contact/quasi_hertz_91.e",
			"path"   : "../data/contact/quasi_hertz_750.e",
			"path_4" : "../data/contact/quasi_hertz_750_smooth.e",
			"path_5" : "../data/contact/quasi_hertz_8732.e",
			"path_6" : "../data/contact/quasi_hertz_8732_smooth.e",
			"order" : 2
		},
		"qp-solver" : {
			"*backend" : "petsc",
			"*type" : "tron",
			"backend" : "any",
			"type" : "ssnewton",
			"max-it" : 8,
			"verbose" : true,
			"linear-solver" : {
				"type" : "direct"
			}
		},
		"space": {
			"system-name": "contact_sol",
			"variables": [
				{
					"name": "disp_x",
					"order" : 2
				},
				{
					"name": "disp_y",
					"order" : 2
				},
				{
					"name": "disp_z",
					"order" : 2
				}
			],
			"boundary-conditions": [
				{
					"var": 0,
					"side": 4,
					"value": 0.0
				},
				{
					"var": 1,
					"side": 4,
					"value": 0.0
				},
				{
					"var": 2,
					"side": 4,
					"value": 0.2
				},

				{
					"var": 0,
					"side": 2,
					"value": 0.0
				},
				{
					"var": 1,
					"side": 2,
					"value": 0.0
				},
				{
					"var": 2,
					"side": 2,
					"value": 0.0
				}
			]
		},
		"model": {
			"*material" : "LinearElasticity",
			"*material"  : "NeoHookean",
			"*stabilization": "L2_LUMPED",
			"stabilization-mag": 0.000001,
			"rescaling" : 1e2,
			"parameters": {
				"mu": 0.3,
				"*lambda": 0.3,
				"poisson-ratio" : 0.1,
				"E" : 100,
				"comment" : "lambda = mu * (E - 2 * mu)/(3 * mu - E) = -0.3"
			}
		},

		"contact": {
			"n-transient-steps": 30,
			"max-nl-iter": 100,
			"radius": 0.5,
			"pairs": [
				{
					"master": 1,
					"slave": 3
				}
			]
		}
	}
}

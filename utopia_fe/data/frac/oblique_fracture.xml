<!-- <solve-strategy>monolithic</solve-strategy> -->

<solve-strategy>staggered</solve-strategy>
<!-- <solve-strategy>separate</solve-strategy> -->
<use-mg>true</use-mg>
<mg-sweeps>1</mg-sweeps>
<mg-levels>4</mg-levels>
<flow-type>transient</flow-type>
<!-- <preset-velocity-field>true</preset-velocity-field> -->

<transport>
	<dt>0.1</dt>
	<simulation-time>1.</simulation-time>
	<lump-mass-matrix>false</lump-mass-matrix>
	<!-- <h1-regularization>false</h1-regularization> -->
	<regularization-parameter>0.5</regularization-parameter>
	<use-upwinding>true</use-upwinding>
	
	<!-- <box>
		<x-min>0.7</x-min>
		<x-max>0.9</x-max>

		<y-min>0.7</y-min>
		<y-max>0.9</y-max>
	</box>
 -->
	<matrix>
		<space>
			<system-name>transport</system-name>
			<variables>
				<var>
					<name>vel_0</name>
					<order>1</order>
					<fe-family>LAGRANGE</fe-family>
				<!-- 	<order>0</order>
					<fe-family>MONOMIAL</fe-family> -->

				</var>
				<var>
					<name>vel_1</name>
					<order>1</order>
					<fe-family>LAGRANGE</fe-family>
				<!-- 	<order>0</order>
					<fe-family>MONOMIAL</fe-family> -->

				</var>
				<var>
					<name>c</name>
					<order>1</order>
					<fe-family>LAGRANGE</fe-family>
				</var>
			</variables>

			<boundary-conditions>
				<entry>
					<side>1</side>
					<value>1</value>
					<var>2</var>
				</entry>
			<!-- 	<entry>
					<side>3</side>
					<value>0</value>
					<var>2</var>
				</entry> -->
			</boundary-conditions>

		</space>

		<forcing-function>
		<!-- 	<fun>
				<side>1</side>
				<value>1.</value>
			</fun> -->
			<!-- <fun>
				<side>4</side>
				<value>-1.</value>
			</fun> -->
			<fun>
				<type>surface</type>
				<value>-1.</value>
				<block>3</block>
				<coord>2</coord>
			</fun>
		</forcing-function>
	</matrix>



</transport>


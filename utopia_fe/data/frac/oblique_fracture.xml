<!-- MASTER STUFF -->
<master>
	<mesh>
		<type>square</type>
		<elem-type>quad</elem-type>
		<order>1</order>

		<min-x>0.</min-x>
		<min-y>0.</min-y>

		<max-x>1.</max-x>
		<max-y>1.</max-y>

		<n-x>20</n-x>
		<n-y>20</n-y>

		<refinements>0</refinements>
	</mesh>

	<space>
		<system-name>master</system-name>

		<variables>
			<var>
			<name>u</name>
			<order>1</order>
			</var>
		</variables>

		<boundary-conditions>
			<entry>
				<side>3</side>
				<value>1</value>
			</entry>
			<entry>
				<side>1</side>
				<value>4</value>
			</entry>
		</boundary-conditions>
	</space>

	<diffusivity>1.</diffusivity>
</master>

<!-- SLAVE STUFF -->
<slave>
	<mesh>
		<type>file</type>
		<path>../data/frac/line.e</path>
		<refinements>1</refinements>
	</mesh>

	<space>
		<system-name>slave</system-name>
		<variables>
			<var>
			<name>u</name>
			<order>1</order>
			<fe-family>LAGRANGE</fe-family>
			</var>
		</variables>

		<boundary-conditions>
			<entry>
				<side>1</side>
				<value>1</value>
			</entry>
			<entry>
				<side>2</side>
				<value>4</value>
			</entry>
		</boundary-conditions>
	</space>

	<forcing-function></forcing-function>

	<diffusivity>1000.</diffusivity>
</slave>


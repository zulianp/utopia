<!-- SLAVE STUFF -->
<slave>
	<mesh>
		<type>file</type>
		<path>../data/frac/midline.e</path>
		<refinements>1</refinements>
	</mesh>

	<sampler>
		<file>../data/frac/aperture.txt</file>
		<min-x>0.</min-x>
		<max-x>1.</max-x>
		<nx>5</nx>
	</sampler>

	<space>
		<system-name>slave</system-name>

		<boundary-conditions>
			<entry>
				<side>1</side>
				<value>1</value>
			</entry>
			<entry>
				<side>2</side>
				<value>4</value>
			</entry>
		</boundary-conditions>
	</space>

	<forcing-function></forcing-function>

	<diffusivity>10.</diffusivity>
</slave>


<transfer>
	<!-- <type>l2-projection</type> -->
	<type>bidirectional-l2-projection</type>

	<function>x</function>
	<!-- <function>1.</function> -->
	<!-- <normalize>true</normalize> -->
	<!-- <bi-op-mass-outside>false</bi-op-mass-outside>  -->
		<use-composite-bidirectional>true</use-composite-bidirectional>
	<quad-order-approx>40</quad-order-approx>
	<!-- MASTER -->
	<master>
		<mesh>
			<type>square</type>
			<elem-type>quad</elem-type>
			<order>2</order>

			<min-x>0.</min-x>
			<min-y>0.</min-y>
			
			<max-x>1.</max-x>
			<max-y>1.</min-y>
	<!-- 		<max-x>0.5</max-x>
			<max-y>0.5</min-y> -->
			
			<n-x>20</n-x>
			<n-y>20</n-y>
		</mesh>
		<space></space>
	</master>

	<!-- SLAVE -->
	<slave>
		<mesh>
			<type>square</type>
			<elem-type>quad</elem-type>
			<order>2</order>

			<min-x>0.</min-x>
			<min-y>0.</min-y>
			
			<max-x>0.5</max-x>
			<max-y>0.5</min-y>

	<!-- 		<max-x>1</max-x>
			<max-y>1</min-y> -->
			
			<n-x>31</n-x>
			<n-y>31</n-y>
		</mesh>
		<space></space>
	</slave>
	
</transfer>

<simulation>
	<mesh>../data/wear/wear_geoms_3.e</mesh>
	<model>
        <material>LinearElasticity</material>
        <!-- <material>NeoHookean</material> -->
		<parameters>
			<mu>600.</mu>
			<lambda>600.</lambda>
		</parameters>
	</model>
	<boundary-conditions>
		<dirichlet type="array">
            <!-- Upper -->
			<entry>
                <coord>0</coord>
                <side>3</side>
                <value>0.</value>
            </entry>
            <entry>
                <coord>1</coord>
                <side>3</side>
                <value>0.</value>
            </entry>
            <entry>
                <coord>2</coord>
                <side>3</side>
                <value>0.</value>
            </entry>
            <!-- Lower -->
            <entry>
                <coord>0</coord>
                <side>4</side>
                <value>0.0</value>
            </entry>
            <entry>
                <coord>1</coord>
                <side>4</side>
                <value>0.0</value>
            </entry>
		</dirichlet>
	</boundary-conditions>
    <forcing-functions type="array">
        <entry>
            <block>2</block>
            <coord>2</coord>
            <value>0.01</value>
        </entry>
    </forcing-functions>
    <contact>
        <type>steady</type>
        <radius>1.5</radius>
        <pairs type="array">
            <entry>
                <master>1</master>
                <slave>2</slave>
            </entry>
        </pairs>
        <step-tol>1e-5</step-tol>
    </contact>
    <wear>
        <n-cycles>100</n-cycles>
        <coeff>0.0007</coeff>
        <extrapolation>5.0</extrapolation>
        <gait-cycle>
            <steps>30</steps>
            <rotations type="array">
                <entry>
                    <block>1</block>
                    <axis>x</axis>
                    <begin>0.</begin>
                    <end>10</end>
                    <!-- steps when active -->
                    <from>0</from>
                    <to>15</to>
                </entry>
                   <entry>
                    <block>1</block>
                    <axis>x</axis>
                    <begin>10.</begin>
                    <end>0.</end>
                    <!-- steps when active -->
                    <from>15</from>
                    <to>30</to>
                </entry>
            </rotations>
            <translations type="array">
                <entry>
                    <block>2</block>
                    <axis>z</axis>
                    <begin>1.</begin>
                    <end>1.</end>
                </entry>
            </translations>
        </gait-cycle>
    </wear>
	<output>.</output>
</simulation>

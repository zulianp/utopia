<simulation>
	<mesh>../data/wear_2_b.e</mesh>
    <time>
        <dt>0.1</dt>
        <steps>100</steps>
    </time>
	<model>
		<material>NeoHookean</material>
		<parameters>
			<mu>300.</mu>
			<lambda>600.</lambda>
		</parameters>
	</model>
	<boundary-conditions>
		<dirichlet type="array">
			<entry>
                <coord>0</coord>
                <side>1</side>
                <value>0.</value>
            </entry>
            <entry>
                <coord>1</coord>
                <side>1</side>
                <value>0.</value>
            </entry>
      <!--       <entry>
                <coord>2</coord>
                <side>1</side>
                <value>0.</value>
            </entry> -->
		</dirichlet>
	</boundary-conditions>
    <forcing-functions type="array">
        <entry>
            <!-- no block means that is applied to all -->
            <!-- <block>1</block> -->
            <coord>1</coord>
            <value>0.1</value>
        </entry>
    </forcing-functions>
    <contact>
        <radius>0.16</radius>
        <pairs type="array">
            <entry>
                <master>1</master>
                <slave>2</slave>
            </entry>
        </pairs>
    </contact>
    <wear>
        <n-cycles>1</n-cycles>
<!--         <worn-surfaces type="array">
            <entry>1</entry>
            <entry>2</entry>
        </worn-surfaces> -->
        <gait-cycle>
            <steps>10</steps>
            <rotations type="array">
                <entry>
                    <block>1</block>
                    <axis>z</axis>
                    <begin>4.0</begin>
                    <end>-1.</end>
                </entry>
            </rotations>
            <translations type="array">
                <entry>
                    <block>1</block>
                    <axis>z</axis>
                    <begin>0.</begin>
                    <end>-1.</end>
                </entry>
            </translations>
        </gait-cycle>
    </wear>
	<output>wear_output</output>
</simulation>

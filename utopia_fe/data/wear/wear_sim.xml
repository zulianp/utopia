<simulation>
	<mesh>../data/wear_2_b.e</mesh>
    <time>
        <dt>0.1</dt>
        <steps>100</steps>
    </time>
	<model>
		<material>NeoHookean</material>
		<parameters>
			<mu>300.</mu>
			<lambda>600.</lambda>
		</parameters>
	</model>
	<boundary-conditions>
		<dirichlet>
			<cond>
                <coord>1</coord>
                <side>1</side>
                <value>1.</value>
            </cond>
		</dirichlet>
	</boundary-conditions>
    <forcing-functions>
        <side>1</side>
        <coord>1</coord>
        <value>0.1</value>
    </forcing-functions>
    <contact>
        <radius>0.1</radius>
        <pair>
            <master>1</master>
            <slave>2</slave>
        </pair>
    </contact>
    <wear>
        <gait-cycle>
            <rotation>
                <block>1</block>
                <axis>z</axis>
                <begin>4.0</begin>
                <end>-1.</end>
            </rotation>
        </gait-cycle>
    </wear>
	<ouput>wear_output</ouput>
</simulation>

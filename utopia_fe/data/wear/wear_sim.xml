<simulation>
	<mesh>../data/wear_2_b.e</mesh>
    <time>
        <dt>0.1</dt>
        <steps>100</steps>
    </time>
	<model>
		<material>NeoHookean</material>
		<parameters>
			<mu>300.</mu>
			<lambda>600.</lambda>
		</parameters>
	</model>
	<boundary-conditions>
		<dirichlet type="array">
			<entry>
                <coord>1</coord>
                <side>1</side>
                <value>1.</value>
            </entry>
		</dirichlet>
	</boundary-conditions>
    <forcing-functions>
        <fun>
            <side>1</side>
            <coord>1</coord>
            <value>0.1</value>
        </fun>
    </forcing-functions>
    <contact>
        <radius>0.16</radius>
        <pairs type="array">
            <entry>
                <master>1</master>
                <slave>2</slave>
            </entry>
        </pairs>
    </contact>
    <wear>
        <gait-cycle>
            <rotation>
                <block>1</block>
                <axis>z</axis>
                <begin>4.0</begin>
                <end>-1.</end>
            </rotation>
        </gait-cycle>
    </wear>
	<output>wear_output</output>
</simulation>
